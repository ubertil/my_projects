#include "include/my.h"
#include <stdlib.h>
#include <time.h>

void	my_amazing_sorter(int *array, int size)
{
  my_putchar('a');
}

int	main(int ac, char **av)
{
  int	tab[my_getnbr(av[1])];
  int	idx;

  idx = 0;
  srand(time(NULL));
  while (idx != my_getnbr(av[1]))
    {
      tab[idx] = rand () % my_getnbr(av[2]);
      my_put_nbr(tab[idx]);
      my_putchar(',');
      idx = idx + 1;
    }
  my_amazing_sorter(tab, my_getnbr(av[1]));
  idx = 0;
  while (idx != my_getnbr(av[1]))
    {
      my_put_nbr(tab[idx]);
      my_putchar(',');
      idx = idx + 1;
    }
  return (0);
}
